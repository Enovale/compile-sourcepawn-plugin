{
  "name": "compile-sourcemod-plugin",
  "description": "Compile sourcemod plugins with the latest 1.11/1.12 build",
  "author": "Sheply <76234443+SheplyRam@users.noreply.github.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src",
    "format": "prettier --write **/*.ts",
    "lint": "eslint **/*.ts",
    "prepare": "husky install"
